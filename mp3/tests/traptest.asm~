SEGMENT CODESEGMENT:
    LEA R0, BULLSHIT
    ADD R1, R0, 1
    TRAP BEAR
    NOP
    NOP
    NOP
    NOP
    NOP
    NOP
    NOP
    NOP
    NOP
    NOP
    AND R1, R0, 0
    BRnzp BAD
    
    
    
    BOOBS:
        LDR R2, R0, GOOD
        BRnzp FIN
        NOP
        NOP
        NOP
        NOP
        NOP
    
    
    
    BAD:
       LDR R2, R0, BADD
       BRnzp FIN
       NOP
       NOP
       NOP
       NOP
       NOP
       
    FIN:
        BRnzp FIN
        NOP
        NOP
        NOP
        NOP
        NOP
        
        
        
    SEGMENT BULLSHIT:
    Bear:	      DATA2 BOOBS    
    GOOD:    DATA2 4X600D
    BADD:    DATA2 4XBADD

